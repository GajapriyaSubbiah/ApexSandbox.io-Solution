public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
    Boolean isPassed, isFailed;
    if((passedExam == true && passedAssignments == true) || (passedExam == true && passedProject == true)){
        return isPassed;
    }else if((passedExam == true && passedAssignments == true) || (passedAssignments == true && passedProject == true)){
        return isPassed;
    }else if((passedExam == true && passedProject == true) || (passedAssignments == true && passedProject == true)){
        return isPassed;
    }else{
        return isFailed;
    }
}