public String aOrAn(String word) {
    //code here
    String result = word.trim();
    if(!String.isBlank(word) && (result.startsWith('a') || result.startsWith('e') || result.startsWith('i') || result.startsWith('o') || result.startsWith('u'))){
        return result = 'an ' + result;
    }else{
        return result = 'a ' + result;
    }
}