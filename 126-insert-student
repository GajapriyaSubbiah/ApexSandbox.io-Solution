public Id insertStudent(String name, String email) {
    // code here
    List<apxio__Student__c> stuList = new List<apxio__Student__c>();
    for(apxio__Student__c stu: stuList){
        apxio__Student__c stud = new apxio__Student__c();
        stud.Name = name;
        stud.apxio__Email__c = email;
        stuList.add(stud);
    }

    if(!stuList.isEmpyt()){
        insert stuList;
    }
}